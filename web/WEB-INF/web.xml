<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GuardarPaciente</servlet-name>
        <servlet-class>Servlet.GuardarPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccionesPaciente</servlet-name>
        <servlet-class>Control.AccionesPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Conexion</servlet-name>
        <servlet-class>Control.Conexion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Paciente</servlet-name>
        <servlet-class>Modelo.Paciente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GuardarPaciente</servlet-name>
        <url-pattern>/GuardarPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccionesPaciente</servlet-name>
        <url-pattern>/AccionesPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Conexion</servlet-name>
        <url-pattern>/Conexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Paciente</servlet-name>
        <url-pattern>/Paciente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
